{
  "name": "pipaslot/modules",
  "description": "Nette modularity library supporting ajax",
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Petr Å tipek",
      "email": "pipaslot@seznam.cz"
    }
  ],
  "keywords": [
    "pipas",
    "nette",
    "modules",
    "ajax"
  ],
  "require": {
    "nette/di": "~2.3",
    "nette/utils": "~2.3",
    "php": ">=5.5.0",
    "composer/composer": "^1.0",
    "nette/application": "^2.3",
    "pipaslot/utils": "^1.1"
  },
  "require-dev": {
    "nette/tester": "~1.3",
    "tracy/tracy": "^2.3",
    "mockery/mockery": "^0.9.4",
    "nette/bootstrap": "^2.3",
    "nette/robot-loader": "^2.3",
    "latte/latte": "^2.3",
    "nette/forms": "^2.3"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "demo/"
    ]
  },
  "extra": {
    "directory": {
      "create": [
        "demo/temp"
      ],
      "clean": [
        "demo/temp"
      ]
    },
    "bower": {
      "files": [
      ],
      "dependencies": {
        "pipaslot-frontend": "~1.*",
        "nette-live-form-validation": "~1.*"
      }
    },
    "grunt": {
    },
    "media": {
      "www-root": "demo",
      "base-path": "media",
      "directories": {
      },
      "ignored": [
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Pipas\\Modules\\Composer\\Extra::run"
    ],
    "post-update-cmd": [
      "Pipas\\Modules\\Composer\\Extra::run"
    ]
  }
}
